import java.util.*;

class GreetingProgram
{
public static void main(String args[])
{
//System.out.println("Enter a number");
//if we not import util package then write as it 
//int num=new java.util.Scanner(System.in).nextInt();
//System.out.println(num);
//if we use import util package then write as it down
System.out.println("Enter a Name");
Scanner sc=new Scanner(System.in);
String name=sc.next();
System.out.println(name);
System.out.println("Enter a your age");
int age=sc.nextInt();
int currentYear=2024;
int birthYear=currentYear-age;
System.out.println("Hello "+name);
System.out.println("your age is "+age);
System.out.println("your birth years is "+birthYear);

}
}
