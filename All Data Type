class AllDataType
{
public static void main(String args[])
{
//declaring a int type variable in local area and assign a value like
int num=10;
int num1=20;
int num2=30;
//if we want to declare another data type value then simply change data type like
double decimalNumber=4.5;
String name="Ramesh Patel";
char ch='r';
boolean is=true;
float decimal=34.6f;
long longData=3673673675765667l;
System.out.println(num);
System.out.println(num1);
System.out.println(num2);
System.out.println(decimalNumber);
System.out.println(name);
System.out.println(ch);
System.out.println(is);
System.out.println(decimal);
System.out.println(longData);
}
}
