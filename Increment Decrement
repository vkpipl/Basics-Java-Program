class IncrementDecrementExample
{
public static void main(String args[])
{
int count=5;
count++;
System.out.println("After increment: "+count);
count--;
System.out.println("After decrement:"+count);
int newCount=count++;
System.out.println("After post-increment:"+count);
System.out.println("Value if newCount:"+newCount);
int anotherCount=--count;
System.out.println("After pre-decrement"+count);
System.out.println("Value of anotherCount"+anotherCount);
}
}
